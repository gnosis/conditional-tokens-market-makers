{
  "name": "@gnosis.pm/hg-market-makers",
  "version": "0.2.0",
  "description": "Collection of Market Makers for Gnosis Mercury",
  "scripts": {
    "lint": "eslint .",
    "develop": "truffle develop",
    "compile": "truffle compile",
    "migrate": "truffle migrate",
    "networks": "truffle networks",
    "test": "truffle test",
    "lint-contracts": "solhint contracts/**/*.sol",
    "injectnetinfo": "tnt iN",
    "extractnetinfo": "tnt eN",
    "resetnetinfo": "truffle networks --clean && tnt iN",
    "measuregasstats": "tnt measureGas -f inheritanceMap.json -o build/gas-stats.json --test-command 'npm test'",
    "prepublishOnly": "eslint . && truffle compile && truffle networks --clean && tnt iN"
  },
  "keywords": [
    "Ethereum",
    "Gnosis",
    "Prediction-Market",
    "Solidity",
    "Truffle"
  ],
  "author": "Gnosis (https://gnosis.pm)",
  "license": "LGPL-3.0",
  "dependencies": {
    "@gnosis.pm/hg-contracts": "^2.0.1",
    "@gnosis.pm/util-contracts": "^2.0.0-alpha.3",
    "erc-1155": "github:cag/erc-1155#sol-0.5",
    "canonical-weth": "^1.3.1",
    "openzeppelin-solidity": "github:elopio/openzeppelin-solidity#solc-nightly-2.0-update-2"
  },
  "devDependencies": {
    "@gnosis.pm/truffle-nice-tools": "^1.1.3",
    "babel-eslint": "^8.0.2",
    "decimal.js": "^10.0.1",
    "eslint": "^4.11.0",
    "eslint-plugin-babel": "^5.1.0",
    "lodash": "^4.17.4",
    "npm-prepublish": "^1.2.3",
    "rlp": "^2.1.0",
    "solhint": "^1.1.10",
    "truffle": "5.0.0-beta.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnosis/hg-automated-market-makers"
  },
  "bugs": {
    "url": "https://github.com/gnosis/hg-automated-market-makers/issues"
  },
  "homepage": "https://github.com/gnosis/hg-automated-market-makers#readme"
}
